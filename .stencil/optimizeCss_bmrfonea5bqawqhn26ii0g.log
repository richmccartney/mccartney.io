:root{--base-font-size:1rem;--base-spacing:1rem;--base-font-family:var(--font-open-sans);--color-white:#fff;--color-off-white:#f0f2f7;--color-light-blue:#b6bcd5;--color-kashmir-blue:#52658f;--color-dark-blue:#333a56;--color-grey:#787d90;--color-dark-grey:#333;--columns:12;--column-gutter:calc(1.5 * var(--base-spacing));--column-margin:calc(2 * var(--base-spacing));--font-open-sans:"Open Sans",sans-serif;--font-playfair-display:"Playfair Display",serif;--font-regular:400;--font-semi-bold:600;--font-bold:700;--max-width:calc(90 * var(--base-spacing))}*,:after,:before{-webkit-box-sizing:border-box;box-sizing:border-box}ol[class],ul[class]{padding:0}blockquote,body,dd,dl,figcaption,figure,h1,h2,h3,h4,li,ol[class],p,ul[class]{margin:0}body{padding:0;min-height:100vh;background:url(/assets/bg.webp) no-repeat center top 250px fixed;background-size:1440px;color:var(--color-grey);font-family:var(--base-font-family);font-weight:var(--font-regular);text-rendering:optimizeSpeed;line-height:1.5;scroll-behavior:smooth}ol[class],ul[class]{list-style:none}a:-webkit-any-link,a:not([class]){color:var(--color-kashmir-blue);-webkit-text-decoration-skip:ink;text-decoration-skip-ink:auto;-webkit-text-decoration-color:var(--color-kashmir-blue);text-decoration-color:var(--color-kashmir-blue)}a:hover,a:visited{color:var(--color-kashmir-blue)}img{max-width:100%;display:block}article>*+*{margin-top:var(--base-spacing)}button,input,select,textarea{font:inherit}@media (prefers-reduced-motion:reduce){*{-webkit-animation-duration:.01ms!important;animation-duration:.01ms!important;-webkit-animation-iteration-count:1!important;animation-iteration-count:1!important;-webkit-transition-duration:.01ms!important;transition-duration:.01ms!important;scroll-behavior:auto!important}}