h1 {
  margin: 0;
}

h5 {
  font-weight: var(--font-semi-bold);
  font-size: calc(0.75 * var(--base-font-size));
  line-height: var(--base-font-size);
  letter-spacing: 1.2px;
  text-transform: uppercase;
  margin: 0;
}

p {
  font-family: Playfair Display;
  font-style: normal;
  font-weight: bold;
  font-size: 64px;
  line-height: 85px;
  
  color: #333A56;
}

a:not([class]) {
  color: var(--color-kashmir-blue);
}