header {
  color: var(--color-dark-blue);
  height: calc(5 * var(--base-spacing));
  padding: 0 var(--column-margin));
  display: -ms-flexbox;
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  border-bottom: 1px solid var(--color-off-white);
}

header img {
  display: block;
}

main {
  padding: 0 var(--column-margin));
}